CREATE TABLE Users (username VARCHAR(64) NOT NULL, role ENUM('model', 'patient', 'physician', 'specialist') NOT NULL, PRIMARY KEY (username));



CREATE TABLE Image (filename VARCHAR(255) NOT NULL, patient VARCHAR(64) NOT NULL, type VARCHAR(64) NOT NULL, AI_eval VARCHAR(36) NOT NULL, human_eval VARCHAR(36), PRIMARY KEY (filename), FOREIGN KEY (AI_eval) REFERENCES Eval(id), FOREIGN KEY (human_eval) REFERENCES Eval(id), FOREIGN KEY (patient) REFERENCES Users(username));

CREATE TABLE Eval (
    id VARCHAR(36) NOT NULL,
    issuer VARCHAR(64) NOT NULL,
    float_rating float,
    class_rating varchar(64),
    comments mediumtext,
    PRIMARY KEY (id),
    FOREIGN KEY (issuer) REFERENCES User(username)
);

sqlalchemy mysqlclient
    

